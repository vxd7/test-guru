<h1>All tests in the database</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th># of questions</th>
  </tr>

  <% @tests.each do |test| %>
    <tr>
      <td><%= test.title %></td>
      <td><%= test.level %></td>
      <td><%= test.questions.count %></td>
    </tr>
  <% end %>
</table>
