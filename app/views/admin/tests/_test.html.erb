<tr>
  <td><%= link_to test.title, admin_test_path(test) %></td>
  <td><%= test.level %></td>
  <td><%= test_category(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= link_to 'Show', admin_test_path(test) %>
  <td><%= link_to 'Edit', edit_admin_test_path(test) %></td>
